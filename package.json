{
  "name": "pokernook",
  "description": "PokerNook Next.js app",
  "repository": "github:pokernook/web",
  "engines": {
    "node": "15.x"
  },
  "scripts": {
    "generate:graphql": "graphql-codegen --require dotenv/config",
    "watch:graphql": "npm run generate:graphql -- --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.ts",
    "dev": "concurrently --kill-others \"next dev\" npm:watch:*",
    "build": "next build",
    "start": "next start -p 8080"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.4.2",
    "@chakra-ui/theme-tools": "^1.1.2",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@urql/devtools": "^2.0.3",
    "@urql/exchange-graphcache": "^4.0.0",
    "date-fns": "^2.19.0",
    "emoji-mart": "^3.0.1",
    "framer-motion": "^4.0.0",
    "graphql": "^15.5.0",
    "next": "^10.1.1",
    "next-urql": "^3.0.0",
    "path-to-regexp": "^6.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-easy-crop": "^3.3.2",
    "react-hook-form": "^7.0.0",
    "react-icons": "^4.2.0",
    "react-is": "^17.0.0",
    "seedrandom": "^3.0.5",
    "urql": "^2.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@graphql-codegen/typescript-urql": "^2.0.5",
    "@types/emoji-mart": "^3.0.4",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/seedrandom": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "concurrently": "^6.0.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
